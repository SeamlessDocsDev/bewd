<p id="notice"><%= notice %></p>

<h1>Listing Forms</h1>

<ul>
  </li>
</ul>


<table>
  <thead>
    <tr>
      <th>Form Name</th>
      <th>Municipality</th>
      <th>State</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms.each do |form| %>
      <tr>
        <td><%= link_to form.form_name, form.form_link %></td>
        <td><%= form.municipality_name %></td>
        <td><%= form.municipality_state %></td>
        <td><%= form.form_department %></td>
        <td><%= link_to 'Show', form %></td>
        <td><%= link_to 'Edit', edit_form_path(form) %></td>
        <td><%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Form', new_form_path %>